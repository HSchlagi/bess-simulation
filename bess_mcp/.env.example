# --- BESS MCP Server Environment (.env) ---
BESS_DB_PATH=/data/bess.db

# MQTT broker
MQTT_BROKER=mqtt-broker
MQTT_PORT=1883
MQTT_USERNAME=
MQTT_PASSWORD=

# Optional: HTTP bind (if you switch from stdio to HTTP/WebSocket)
HOST=0.0.0.0
PORT=8080

# --- Dispatch adapter (plug your real code here) ---
DISPATCH_MODULE=app.sim_dispatch   # Python module path
DISPATCH_FUNC=run_dispatch         # Function that receives a dict and returns KPIs as dict

# aWATTar settings (AT or DE); day format YYYY-MM-DD
AWATTAR_BASE_URL=https://api.awattar.at/v1/marketdata
AWATTAR_COUNTRY=AT
